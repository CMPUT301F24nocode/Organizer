<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/rounded_overlay"
    android:backgroundTint="@color/lucky_uiEmphasis">


    <TextView
        android:id="@+id/textView11"
        style="@style/GenericText"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:drawablePadding="4dp"
        android:fontFamily="@font/airbnbcereal_w_bd"
        android:text="@string/organiser_profile_list_object_name"
        android:textAlignment="viewEnd"
        app:drawableLeftCompat="@drawable/entrant_icon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/send_notification_profile_button"
        style="@style/ActiveButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/airbnbcereal_w_bd"
        android:padding="8dp"
        android:text="Send Notificaton"
        android:textAlignment="textStart"
        android:textAllCaps="false"
        android:textSize="16sp"
        android:visibility="visible"
        app:cornerRadius="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <Button
        android:id="@+id/cancel_entrant_profile_button"
        style="@style/ActiveButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:backgroundTint="@color/lucky_red_alert"
        android:fontFamily="@font/airbnbcereal_w_bd"
        android:padding="8dp"
        android:text="Cancel Entrant"
        android:textAlignment="textStart"
        android:textAllCaps="false"
        android:textSize="16sp"
        android:visibility="visible"
        app:cornerRadius="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />


</androidx.constraintlayout.widget.ConstraintLayout>